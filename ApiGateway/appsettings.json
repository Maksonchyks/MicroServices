{
    "ReverseProxy": {
        "Routes": {
            "orders-route": {
                "ClusterId": "orders-cluster",
                "Match": {
                    "Path": "/api/orders/{**catch-all}"
                },
                "Transforms": [
                    { "PathPattern": "{**catch-all}" }
                ]
            },
            "catalog-route": {
                "ClusterId": "catalog-cluster",
                "Match": {
                    "Path": "/api/catalog/{**catch-all}"
                },
                "Transforms": [
                    { "PathPattern": "{**catch-all}" }
                ]
            },
            "reviews-route": {
                "ClusterId": "reviews-cluster",
                "Match": {
                    "Path": "/api/reviews/{**catch-all}"
                },
                "Transforms": [
                    { "PathPattern": "{**catch-all}" }
                ]
            },
            "aggregator-route": {
                "ClusterId": "aggregator-cluster",
                "Match": {
                    "Path": "/api/aggregator/{**catch-all}"
                },
                "Transforms": [
                    { "PathPattern": "{**catch-all}" }
                ]
            }
        },
        "Clusters": {
            "orders-cluster": {
                "Destinations": {
                    "orders-destination": {
                        "Address": "https://localhost:7148"
                    }
                }
            },
            "catalog-cluster": {
                "Destinations": {
                    "catalog-destination": {
                        "Address": "https://localhost:7267"
                    }
                }
            },
            "reviews-cluster": {
                "Destinations": {
                    "reviews-destination": {
                        "Address": "https://localhost:7097"
                    }
                }
            },
            "aggregator-cluster": {
                "Destinations": {
                    "aggregator-destination": {
                        "Address": "https://localhost:7118"
                    }
                }
            }
        }
    }
}